<div class="col-12 col-sm-4">
  <div class="card-body">
    <a href="/asteroid/{{id}}" class="text-decoration-none">

      <h3 class="card-title">
        {{name}}
      </h3>
    </a>

    <div class="card-data">

      <p class="result-title">Lunar Distance:</p>
      <p class="card-result">{{miss_distance}}</p>

      <p class="result-title">Hazardous:</p>
      <p class="card-result">{{hazardous}}</p>

    </div>

  </div>

</div>